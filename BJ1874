#include <stdio.h>
#define MAX 100001

int arr[MAX];
int stack[MAX];
int top=0;
char stack_ch[MAX*2];
int top_ch=0;

void push(int key)
{
    stack[top] = key;
    stack_ch[top_ch] = '+';
    top++;
    top_ch++;
}

void pop()
{
    top--;
    stack_ch[top_ch] = '-';
    top_ch++;
}

//임의의 수의 좌,우 값이 임의의 수보다 크면 NO
int main(void)
{
    int num;
    scanf("%d",&num);
    int check=1;

    for(int i=0;i<num;i++)
    {
        scanf("%d",&arr[i]);
    }

    for(int k=0;k<num;k++)
    {
        if(k==0)
        {
            for(int x=0;x<arr[k];x++)
            {
                push(arr[x]);
            }

            pop();
        }

        else
        {
            if(arr[k]<arr[k-1])
            {   
                if(arr[k]!=stack[top])
                {
                    check = 0;
                    break;
                }

                else
                {
                    pop();
                }
            }

            else if (arr[k]>arr[k-1])
            {
                for(int x=arr[k-1];x<arr[k];x++)
                {
                    push(arr[x]);
                }

                pop();
            }
        }
    }

    for(int j=0;j<top_ch;j++)
    {
        if(check == 0)
        {
            printf("NO\n");
            break;
        }

        else
        {
            printf("%c\n",stack_ch[j]);
        }
    }

    return 0;
}
