//자료구조 1차 코딩테스트 2번 답안
#include <stdio.h>
#include <stdlib.h>

typedef struct Node{
	int key;
	struct Node* link;
}Node;

int main(void)
{
	int n;
	scanf("%d",&n);

	Node* headNode =(Node*)malloc(sizeof(Node));
	headNode->key = -9999;
	headNode->link = NULL; 

	//연결리스트 
	for(int i=0;i<n;i++)
	{
		Node* newNode = (Node*)malloc(sizeof(Node));
		newNode->key = i+1;
		newNode->link = NULL;
		Node* temp = headNode->link;

		if(temp == NULL)
		{
			headNode->link = newNode;
		}

		else
		{
			while(temp->link != NULL)
			{
				temp = temp->link;
			}

			temp->link = newNode;
		}
	}

	Node* curr = headNode->link;
	Node* next = NULL;

	//연결리스트 출력
	while(curr != NULL)
	{
		printf("%d ",curr->key);
		curr = curr->link;
	}

	//연결리스트 free
	curr = headNode->link;
	while(curr!=NULL)
	{
		next = curr;
		curr = curr->link;
		free(next);
	}

	return 0;
}
