//자료구조 1차 코딩테스트 2번 답안
#include <stdio.h>
#include <stdlib.h>

typedef struct Node{
	int key;
	struct Node* link;
}Node;

int main(void)
{
	int n;
	int count = 1;
	scanf("%d",&n);

	Node** List = (Node**)malloc(sizeof(Node*)*n);

	for(int i=0;i<n;i++)
	{
		List[i] = (Node*)malloc(sizeof(Node));
	}

	for(int j=0;j<n;j++)
	{
		List[j]->key = count++;
		if(j == n-1)
		{
			List[j]->link = NULL;
		}
		List[j]->link = List[j+1];
	}

	Node* temp = List[0];

	for(int k=0;k<n;k++)
	{
		printf("%d ",temp->key);
		if(List[k]->link==NULL)
		{
			break;
		}
		temp = temp->link;
	}

	for(int l = 0;l<n;l++)
	{
		free(List[l]);
	}

	free(List);

	return 0;
}
